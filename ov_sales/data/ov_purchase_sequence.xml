<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_purchase_order1" model="ir.sequence">
            <field name="name">Request For Quotation</field>
            <field name="code">purchase.order1</field>
            <field name="prefix">PR-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order2</field>
            <field name="prefix">PO-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>