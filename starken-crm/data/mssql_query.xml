<odoo>
    <data>
        <record id="vpn_fail_email_template_products_notf_email" model="mail.template">
            <field name="name">VPN : Failed to Sync Product Email Template</field>
            <field name="model_id" ref="starken-crm.model_starken_crm_mssql_query"/>
            <field name="email_from"></field>
            <field name="partner_to"></field>
            <field name="subject"></field>
            <field name="body_html" type="html">
                <div style="margin: 0px; padding: 0px;">
                    <p style="margin: 0px; padding: 0px; font-size: 13px;">
                        Greetings,
                        <br /><br />
                        Unfortunately, the synchronization of the products with Epicor System using VPN has failed with the error below :-
                        <br /><br />
                        ${ctx.get('trace_msg')}
                        <br /><br />
                        <br /><br />
                    </p>
                </div>
            </field>
            <field name="user_signature" eval="False"/>
            <field name="auto_delete" eval="True"/>
        </record>
        <record id="vpn_products_link_schedule" model="ir.cron">
            <field name="name">Link products from MSSQL</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="state">code</field>
            <field name="model_id" ref="starken-crm.model_starken_crm_mssql_query"/>
            <field name="code">model.sync_product_tracked_from_database()</field>
            <field name="priority" eval="5" />
        </record>
        <record id="vpn_uom_sync" model="ir.cron">
            <field name="name">Sync: UOM</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 01:05:00')" />
            <field name="doall">1</field>
            <field name="state">code</field>
            <field name="model_id" ref="starken-crm.model_starken_crm_uom_sync"/>
            <field name="code">model.sync_product_uom()</field>
            <field name="priority" eval="5" />
        </record>
        <record id="vpn_customer_sync" model="ir.cron">
            <field name="name">Sync: Customer</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 01:05:00')" />
            <field name="doall">1</field>
            <field name="state">code</field>
            <field name="model_id" ref="starken-crm.model_starken_crm_mssql_query"/>
            <field name="code">model.sync_customer_tracked_from_database()</field>
            <field name="priority" eval="5" />
        </record>
        <record id="vpn_shipto_sync" model="ir.cron">
            <field name="name">Sync: Ship To</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 01:35:00')" />
            <field name="doall">1</field>
            <field name="state">code</field>
            <field name="model_id" ref="starken-crm.model_starken_crm_mssql_query"/>
            <field name="code">model.sync_shipto_tracked_from_database()</field>
            <field name="priority" eval="5" />
        </record>
    </data>
</odoo>